OPENAI_API_KEY=your_openai_key
OPENAI_MODEL=gpt-4.1-mini
ENABLE_OPENAI_TRACE=false
MAX_HISTORY_MESSAGES=12

DATABRICKS_HOST=adb-xxxx.azuredatabricks.net
DATABRICKS_TOKEN=your_databricks_pat
DATABRICKS_WAREHOUSE_ID=your_sql_warehouse_id

# Server performance/safety tuning
DATABRICKS_HTTP_TIMEOUT_SECONDS=30
DATABRICKS_POLL_TIMEOUT_SECONDS=60
DATABRICKS_POLL_INTERVAL_SECONDS=0.5
DATABRICKS_WAIT_TIMEOUT_SECONDS=10
DATABRICKS_MAX_ROWS=500
DATABRICKS_MAX_ROWS_HARD_LIMIT=5000
DATABRICKS_METADATA_CACHE_TTL_SECONDS=60
